{
  "private": true,
  "name": "homes-site",
  "version": "0.0.0",
  "description": "Homes To Love web site",
  "main": "start.js",
  "engines": {
    "node": ">=6.1.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "prepush": "run-p lint test",
    "start": "node ./start.js",
    "start:debug": "node $NODE_DEBUG_OPTION ./start.js",
    "test": "nyc --include-all-sources --report-dir ../shippable/codecoverage -r=cobertura -r=html mocha --require @babel/polyfill --require @babel/register --require ./test/setup.js --recursive --check-leaks",
    "test:debug": "nyc --include-all-sources -r=cobertura -r=html -r=text -r=html mocha --require @babel/polyfill --require @babel/register --require ./test/setup.js --recursive --check-leaks",
    "test:ci": "npm run test -- --reporter=xunit --reporter-options output=./../shippable/testresults/result.xml",
    "test:ci:debug": "npm run test:debug -- --reporter=xunit --reporter-options output=./../shippable/testresults/result.xml",
    "test:unit": "mocha --require babel-polyfill --require babel-register --require test/setup.js --reporter spec --recursive --check-leaks",
    "test:tdd": "npm run test:unit -- --watch",
    "clean": "rimraf dist/ coverage/ && mkdir dist",
    "build": "npm run clean && NODE_ENV=production webpack --bail --hide-modules --progress --mode production",
    "dev": "npm run clean && webpack --bail --debug --display-error-details --hide-modules --progress --watch --mode development & npm run serve",
    "dev:debug": "npm run clean && webpack --bail --debug --display-error-details --hide-modules --progress --watch --mode development & npm run serve:debug",
    "dev:prod": "run-p build start:debug",
    "serve": "nodemon --watch app/server --watch dist --ext js,css ./start.js",
    "serve:debug": "nodemon $NODE_DEBUG_OPTION --watch app --ext js ./start.js",
    "lint:scss": "sass-lint --config .sass-lint.yml --ignore './**/vendor/**' --no-exit --verbose './app*/styles/**/*.scss'",
    "fmt:js": "prettier --write './*.js' 'app/**/*.js' 'test/**/*.js' 'automation/**/*.js' 'stubs/**/*.js'",
    "lint:js": "eslint --fix --ext .js --ext .jsx app/",
    "lint": "run-p lint:scss fmt:js lint:js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bauerxcelmedia/homes-site.git"
  },
  "author": "Bauer Xcel Media",
  "license": "UNLICENSED",
  "homepage": "https://github.com/bauerxcelmedia/homes-site#readme",
