{
"family": "cci-task4",
"requiresCompatibilities": [ "EC2" ],
"executionRoleArn": "arn:aws:iam::014901580825:role/ecsTaskExecutionRole",
"containerDefinitions":
    [
      {
            "name": "cci-container4",
            "image": "317367993082.dkr.ecr.ap-southeast-2.amazonaws.com/cci-repos2:latest",
            "cpu": 512,
            "memory": 512,
            "memoryReservation": 512,
            "portMappings":
            [
                {
                "containerPort": 3001,
                "hostPort": 80,
                "protocol": "tcp"
                }
            ],
            "environment":
            [
                {
                "name": "NODE_ENV",
                "value": "production"
                },
                {
                "name": "APP_ENV",
                "value": "test"
                },
                {
                "name": "APP_STUBBED",
                "value": "true"
                },
                {
                "name": "APP_DEBUG",
                "value": "true"
                }
            ],
            "logConfiguration":
            {
                "logDriver": "awslogs",
                "options":
                {
                    "awslogs-group": "/ecs/cci-app-ecs-logs",
                    "awslogs-region": "ap-southeast-2",
                    "awslogs-stream-prefix": "ecs-cci-task4"
                }
            },
            "essential": true
        }
    ],
"cpu": "1024",
"memory": "1024"
}
