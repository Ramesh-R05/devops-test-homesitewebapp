{
    "family": "cci-task3",
    "executionRoleArn":  arn:aws:iam::014901580825:role/ecsTaskExecutionRole
    "containerDefinitions": [
        {
            "name": "cci-task3",
            "image": "317367993082.dkr.ecr.ap-southeast-2.amazonaws.com/cci-repos2:latest",
            "cpu": 1024,
            "memoryReservation": 1024,
            "portMappings": [
              {
                    "containerPort": 3001,
                    "hostPort": 80,
                    "protocol": "tcp"
              }
            ],
            "command": [
                "./main"
            ],
            "essential": true,
            "environment": [
                   {
                       "name": "NODE_ENV",
                       "value": "production"
                   },
                   {
                       "name": "APP_ENV",
                       "value": "test"
                   },
                   {
                       "name": "APP_STUBBED",
                       "value": "true"
                   },
                   {
                       "name": "APP_DEBUG",
                       "value": "true"
                   }
               ],
               "mountPoints": [],
               "volumesFrom": [],
               "logConfiguration": {
                   "logDriver": "awslogs",
                   "options": {
                       "awslogs-group": "/ecs/cci-task2",
                       "awslogs-region": "ap-southeast-2",
                       "awslogs-stream-prefix": "ecs"
                   }
       ],
        }
    ]
}
